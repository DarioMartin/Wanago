<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentEventsList">

    <fragment
        android:id="@+id/fragmentEventsList"
        android:name="com.architectcoders.wanago.presentation.FragmentEventsList"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_events_list">
        <action
            android:id="@+id/action_fragmentEventsList_to_eventDetailsFragment"
            app:destination="@id/eventDetailsFragment" />
    </fragment>

    <fragment
        android:id="@+id/eventDetailsFragment"
        android:name="com.architectcoders.wanago.presentation.EventDetailsFragment"
        android:label="@string/event_details"
        tools:layout="@layout/fragment_event_details" >
        <argument
            android:name="eventId"
            app:argType="string" />
    </fragment>
</navigation>